@Model Processor
    <div id="myCarousel" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="img-pc" src="~/img/ProcessorAva.jpg" style="width:100%;" />
                <div class="container">
                    <div class="carousel-caption text-start">
                        <h1>@Model.processorCategory</h1>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="rectr" id="d2"></div>
    <div class="row mt-5 mb-2">

        @{
            foreach (Processor processor in Model.allprocessors)
            {

                <div id="box1" class="box" style="width: 300px">
                    <img id="imagen" class="displayed" src="@processor.img" style="max-width:200px; display:block; margin: 0px auto" />
                    <h4>@processor.name  @processor.model @processor.hhz Ггц</h4>
                    <h5>@processor.producer  <font size="3">@processor.socket</font></h5>
                    <h6>Ядра: @processor.yadra    Потоки: @processor.potoks</h6>
                    <h6>TDP: @processor.TDP W</h6>
                    <p><a class="btn btn-warning" href="#">Добавити</a></p>
                </div>



            }
            
        }
    </div>
